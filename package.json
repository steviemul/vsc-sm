{
	"name": "vsc-sm",
	"displayName": "vsc-sm",
	"description": "OCC VS Code Integration",
	"version": "0.0.1",
	"publisher": "steviemul",
	"license": "MIT",
	"engines": {
		"vscode": "^1.32.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"*"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "extension.helloWorld",
				"title": "Hello World"
			},
			{
				"command": "extension.openComponentCode",
				"title": "Open Component Code",
				"icon": {
					"light": "images/code_16.png",
					"dark": "images/code_16.png"
				}
			},
			{
				"command": "occ.deploy",
				"title": "Deploy Application",
				"icon": {
					"light": "images/arrow-up.png",
					"dark": "images/arrow-up.png"
				}
			}
		],
		"viewsContainers": {
			"activitybar": [
				{
					"id": "occ-explorer",
					"title": "Oracle Commerce Cloud",
					"icon": "images/commerce_w_28.png"
				}
			]
		},
		"views": {
			"occ-explorer": [
				{
					"id": "dsApplications",
					"name": "Design Studio Applications"
				},
				{
					"id": "dsLayouts",
					"name": "Design Studio Layouts"
				},
				{
					"id": "dsComponents",
					"name": "Design Studio Components"
				}
			]
		},
		"menus": {
			"view/item/context": [
				{
					"command": "occ.deploy",
					"when": "view == dsApplications",
					"group": "inline"
				},
				{
					"command": "extension.openComponentCode",
					"when": "view == dsComponents",
					"group": "inline"
				}
			]
		}
	},
	"scripts": {
		"vscode:prepublish": "yarn run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"postinstall": "node ./node_modules/vscode/bin/install",
		"test": "yarn run compile && node ./node_modules/vscode/bin/test"
	},
	"devDependencies": {
		"@types/mocha": "^2.2.42",
		"@types/node": "^10.12.21",
		"tslint": "^5.12.1",
		"typescript": "^3.3.1",
		"vscode": "^1.1.28"
	},
	"dependencies": {
		"@babel/parser": "^7.4.5",
		"@babel/types": "^7.4.4",
		"@types/glob": "^7.1.1",
		"axios": "^0.18.0",
		"babel-core": "^6.26.3",
		"glob": "^7.1.3"
	}
}
